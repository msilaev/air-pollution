{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iam:CreateRole",
        "iam:DeleteRole",
        "iam:GetRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:PutRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:GetRolePolicy",
        "iam:PassRole",
        "logs:ListTagsForResource",
        "logs:TagResource",
        "logs:UntagResource",
        "rds:ModifyDBInstance"
      ],
      "Resource": "*"
    }
  ]
}
